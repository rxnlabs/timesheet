{
	"name": "deyonte-basic",
	"version": "1.0.0",
	"private": true,
	"description": "Basic assets builder and transpiler for JS and CSS files. Compiles TypeScript and uses Eslint.",
	"engines": {
		"node": ">=16.x <=27.x",
		"npm": ">=8.x <=14.x"
	},
	"scripts": {
		"watch": "npm run styles -- --ext css --watch & npm run styles -- --ext min.css --env production --watch & npm run scripts -- --watch && kill $!",
		"dev": "npm run react",
		"build": "npm run react || npm run html",
		"styles": "postcss _assets_src/scss/*.scss _assets_src/scss/**/*.scss _assets_src/scss/*.css _assets_src/scss/**/*.css _assets_src/css/*.css _assets_src/css/**/*.css !_assets_src/scss/_*.scss !_assets_src/scss/**/_*.scss !_assets_src/scss/_*.css !_assets_src/scss/**/_*.css !_assets_src/css/_*.css !_assets_src/css/**/_*.css --dir build/css --map --verbose",
		"scripts": "vite build --config vite.config.mjs",
		"lint": "npx eslint \"app/**/*.{js,jsx,ts,tsx,mjs,mjsx}\"",
		"lint:fix": "npx eslint \"app/**/*.{js,jsx,ts,tsx,mjs,mjsx}\" --fix"
	},
	"author": "De'YontÃ© Wilkinson",
	"license": "GPL-3.0",
	"browserslist": {
		"development": [
			"last 1 Chrome version",
			"last 1 Firefox version",
			"last 1 Safari version"
		],
		"production": [
			"last 5 versions",
			"> 0.1%",
			"not dead"
		]
	},
	"devDependencies": {
		"@babel/preset-react": "^7.26.3",
		"@csstools/postcss-sass": "^5.1.1",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.19",
		"cssnano": "^7.0.1",
		"eslint": "^9.22.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.4",
		"fast-glob": "^3.3.2",
		"globals": "^16.0.0",
		"postcss-cli": "^11.0.0",
		"postcss-import": "^16.1.0",
		"postcss-scss": "^4.0.9",
		"sass": "^1.75.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^6.2.0",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"dependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	}
}
